#include <stdio.h>
#include <stdlib.h>

int main(){
	int array[3]={3,32,1};
	int i,j,k,aux;
	
	for(i = 0; i<3; ++i){
	
		for(j = 0; j<3; ++j){
			if(array[j]>array[j+1]){
				aux = array[j];
				array[j] = array[j+1];
				array[j+1] = aux;
			}
	
		}
	}
	for(k= 0; k<3; ++k){
		printf("%i ", array[k]);
	
	}
	
	
	return 0;
}

